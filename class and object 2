Class Student{
 string name;
 string gender;
 int age;

public void getInfo{
   System.out.println("name of this student" + this.name);
   System.out.println("gender of this student" + this.gender);
   System.out.println("age of this student" + this.age);
}
}
public class OOPS{
public staic void main(String args[]){
  Student S1 = new student();
  S1.name = "Mousam";
  S1.gender = female;
